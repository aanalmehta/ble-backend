{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "max-line-length": false,
    "ordered-imports": false,
    "no-string-literal": false,
    "member-ordering": false,
    "interface-name": [
      true,
      "never-prefix"
    ]
  },
  "rulesDirectory": []
}